

<html>
    <head>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">
        <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    </head>

    <table id="crafting_prices" class="display" style="width:100%">
        <thead>
        <tr>
            <th> Name </th>
            <th> Cheapest </th>
            <th> Cheapest HQ </th>
            <th> Price to craft </th>
            <th> Profit </th>
            <th> Average history </th>
            <th> Sold in past week </th>
            <th> Historical profit </th>
        </tr>
        </thead>
        <tbody>
        {{#models}}
            <tr>
                <td> {{name}} </td>
                <td> {{cheapest}} </td>
                <td> {{cheapestHQ}} </td>
                <td> {{priceToCraft}} </td>
                <td> {{profit}} </td>
                <td> {{averageHistory}} </td>
                <td> {{numSoldInPastWeek}} </td>
                <td> {{historicalProfit}} </td>
            </tr>
        {{/models}}

        </tbody>
    </table>
    <div style="text-align:center; font-family:Verdana; color:dimgray;">Last updated on {{lastUpdatedOn}}</div>
    <script>
        $(document).ready(function() {
            $('#crafting_prices').DataTable( {
                "order": [[ 7, "desc" ]],
                stateSave: true,
                "pageLength": 20
            } );
        } );
    </script>
</html>









